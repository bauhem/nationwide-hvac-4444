<% content_for(:head) do %>
  <%= dato_meta_tags(dato.beat) %>
   <link href="<%= current_page.url %>" rel="canonical" />
<% end %>


<div class="section-hero-interior">
  <h1 class="cta-heading" data-ix="hero-appear-1"> <%= dato.beat.title %></h1>
</div>
<div class="section-about">
  <div class="msp-holder">
    <div class="form-beat-quote second w-form">
      <form id="beat-my-quote" name="wf-form-beat-my-quote" data-name="beat-my-quote" method="post" action="/thank-you" data-netlify="true" netlify-honeypot="bot-field" class="w-clearfix">
        <div class="div-tow-collumn">
          <div class="div-40 form">
            <%= dato.beat.description %>
          </div>
          <div class="div-50-2 form">
            <div class="div-flex-h-2 no-min-height"><input type="text" id="first-name" name="first-name" data-name="first-name" placeholder="First Name" maxlength="256" class="text-field-2 bigger-field added-right-margin w-input"><input type="text" id="second-name" name="second-name" data-name="second-name" placeholder="Second Name" maxlength="256" required="" class="text-field-2 bigger-field w-input"></div><input type="email" id="email-3" name="email" data-name="email" required="" maxlength="256" placeholder="Email Address" class="text-field-2 bigger-field w-input">
            <input type="text" id="bot-field" name="Bot-field" data-name="Bot-field" placeholder="Bot-field" maxlength="256" class="text-field-2 bigger-field bot-field w-input"><input type="text" id="Telephone" name="Telephone" data-name="Telephone" placeholder="Phone Number" maxlength="256" required="" class="text-field-2 bigger-field w-input"><textarea id="message" name="message" placeholder="Message" maxlength="5000" data-name="message" class="textarea-2 w-input"></textarea>
            <div class="html-left w-embed"><input type="file" class="custom-file-input" name="file" id="file"></div>
            <div netlify-recaptcha></div>
          </div>
        </div>
        <div class="div-spacer-2"></div><input type="submit" data-wait="Please wait..." value="SEND" class="submit final w-button"></form>
      <div class="w-form-done">
        <div>Thank you! Your submission has been received!</div>
      </div>
      <div class="w-form-fail">
        <div>Oops! Something went wrong while submitting the form</div>
      </div>
    </div>
  </div>
</div>

<%= partial "/partials/section-cta" %>
