  <%= yield_content :meta %>
  <%= auto_display_meta_tags %>
  <%= google_analytics_tag %>

  <link href="https://cdn.snipcart.com/themes/2.0/base/snipcart.min.css" type="text/css" rel="prefetch">

  <!-- [if lt IE 9]>
